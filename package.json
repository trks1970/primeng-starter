{
    "name": "primeng-starter",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "openapi": "openapi-generator-cli generate -i ./api/api.yaml -g typescript-angular -o ./generated/openapi --additional-properties fileNaming=kebab-case,ngVersion=17.0.0 --generate-alias-as-model",
        "prebuild": "npm run openapi",
        "prestart": "npm run openapi",
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^17.2.0",
        "@angular/common": "^17.2.0",
        "@angular/compiler": "^17.2.0",
        "@angular/core": "^17.2.0",
        "@angular/forms": "^17.2.0",
        "@angular/platform-browser": "^17.2.0",
        "@angular/platform-browser-dynamic": "^17.2.0",
        "@angular/router": "^17.2.0",
        "@azure/msal-angular": "^3.0.13",
        "@azure/msal-browser": "^3.10.0",
        "@ngneat/transloco": "^6.0.4",
        "flag-icons": "^7.1.0",
        "primeflex": "^3.3.1",
        "primeicons": "^6.0.1",
        "primeng": "^17.8.0",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.14.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^17.2.0",
        "@angular/cli": "^17.2.0",
        "@angular/compiler-cli": "^17.2.0",
        "@openapitools/openapi-generator-cli": "^2.9.0",
        "@types/jest": "^29.5.10",
        "eslint": "^8.39.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "latest",
        "eslint-plugin-jsdoc": "latest",
        "eslint-plugin-prefer-arrow": "latest",
        "jest": "^29.7.0",
        "jest-junit": "^16.0.0",
        "jest-preset-angular": "^13.1.4",
        "karma-coverage": "^2.2.1",
        "typescript": "~5.2.2"
    },
    "jest": {
        "preset": "jest-preset-angular",
        "setupFilesAfterEnv": [
            "<rootDir>/setup-jest.ts"
        ],
        "globalSetup": "jest-preset-angular/global-setup",
        "reporters": [
            "default",
            "jest-junit"
        ],
        "collectCoverage": true,
        "coverageReporters": [
            "cobertura",
            "lcov"
        ],
        "globals": {
            "crypto": "require('crypto')"
        }
    },
    "jest-junit": {
        "outputDirectory": "reports",
        "outputName": "TEST.xml"
    }
}
